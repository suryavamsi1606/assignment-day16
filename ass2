#include<stdio.h>
main()
{
    int n,i=1,k,s1=0,s2=0;
    printf("enter value of n\n");
    scanf("%d",&n);
    printf("enter value of k\n");
    scanf("%d",&k);
    while(i<=n)
    {
       if(i%k==0)
	   {
		   printf("%d\n",i);  
	       if(i%2==0)
	       {
		       s1=s1+i;
	       }
	       else
	       {
		       s2=s2+i;
	       }
        }
        i++;
    }
    printf("even sum=%d\t",s1);
    printf("odd sum=%d",s2);
}
